<Ball>:
	size_hint: None, None
	canvas:
		Color:
			rgba: 1, 0, 0, 1
		Ellipse:
			size: self.size
			pos:self.pos
<ClockCircle>:
	ball: ball
	canvas:
		Line:
			circle: self.center_x, self.center_y, 130
			width: 5.0
	Button:
		size_hint: None, None
		text: 'Start'
		on_press: root.start()
	Ball:
		id: ball
		size: 10, 10
		pos: self.parent.center_x - 5, self.parent.center_y + 125
